{
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/public/favicon.ico",
      "headers": {
        "cache-control": "public, max-age=31536000"
      },
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/logo(.*).png",
      "headers": {
        "cache-control": "public, max-age=31536000"
      },
      "dest": "/logo$1.png"
    }
  ]
}
